{% extends 'base.jinja' %}

{% block title %} Users {% endblock %}

{% block content %}

<h2>Users</h2>

<ul>
{% for user in users%}
    <li><a href="/users/{{ user.id }}">{{ user }}</a></li>
{% endfor %}
</ul>

<p>
<a class="btn btn-secondary" href="/users/new" role="button">Add user</a>
</p>

{% endblock %}